 <%= simple_form_for(@product) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :gender %>
  <%= f.input :price_cents %>
  <%= f.input :category %>
  <%= f.input :description %>
  <%= f.input :season %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit %>
<% end %>

